---
import home from '../data/home.json'
import Layout from '../layouts/Layout.astro'
import Header from '../components/core/layout/Header.astro'
import Catalog from '../components/home/layout/Catalog.astro'
const { catalogs } = home
---

<Layout>
  <Header title={home.header.title} description={home.header.description} />
  {
    catalogs.map((catalog) => (
      <Catalog title={catalog.title} description={catalog.description} link={catalog.link} imgSrc={catalog.imgSrc} background={catalog.background} articles={catalog?.articles} />
    ))
  }
  <footer></footer>
</Layout>
